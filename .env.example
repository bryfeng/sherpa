# Agentic Wallet Configuration

# Core API Settings
PORT=8000
HOST=127.0.0.1
LOG_LEVEL=INFO

# External API Keys
ALCHEMY_API_KEY=your_alchemy_key_here
COINGECKO_API_KEY=your_coingecko_key_here
SOLANA_HELIUS_API_KEY=your_helius_key_here
AWS_S3_EXPORT_BUCKET=sherpa-history-exports
REDIS_URL=redis://localhost:6379/0

# Cache Settings
CACHE_TTL_SECONDS=300
MAX_CACHE_SIZE=1000

# Rate Limiting
MAX_CONCURRENT_REQUESTS=10
REQUEST_TIMEOUT_SECONDS=30

# Provider Settings
ENABLE_ALCHEMY=true
ENABLE_COINGECKO=true

# ERC-4337 (EVM AA)
ENABLE_ERC4337=false
ENABLE_ALCHEMY_WALLET_API=false
ERC4337_ENTRYPOINT_ADDRESS=
ERC4337_BUNDLER_URL=
# Optional per-chain override (JSON map): {"1":"...","8453":"...","42161":"..."}
ERC4337_BUNDLER_URLS=
# Optional override (defaults to https://api.g.alchemy.com/v2/{ALCHEMY_API_KEY})
ALCHEMY_WALLET_API_URL=

# LLM Providers
LLM_PROVIDER=anthropic
ANTHROPIC_API_KEY=
OPENAI_API_KEY=
Z_API_KEY=
LLM_MODEL=claude-3-sonnet-20240229

# Rhinestone (Smart Wallet + Intent Infrastructure)
ENABLE_RHINESTONE=false
RHINESTONE_API_KEY=
# RHINESTONE_BASE_URL=https://api.rhinestone.dev

# Convex Database
CONVEX_URL=https://your-deployment.convex.cloud
CONVEX_DEPLOY_KEY=prod:your-deploy-key
CONVEX_INTERNAL_API_KEY=your-internal-api-key-for-http-actions
